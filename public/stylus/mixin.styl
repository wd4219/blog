header-color = #20A0FF
main-color= #D3DCE6
border-radius(n)
  border-radius n
box-shadow(n)
  box-shadow 0 0 n #99A9BF
@keyframes draw-nav-item 
  0% 
    stroke-dasharray: 100,600
    stroke-dashoffset: -215
    stroke-width: 8px
  100% 
    stroke-dasharray: 400
    stroke-dashoffset: 0
    stroke-width: 2px

@keyframes offset {
  to {
    stroke-dashoffset: 0px;
  }
}
.clearfix:after
  content: '.'
  height: 0
  visibility: hidden
  clear: both
  overflow: hidden