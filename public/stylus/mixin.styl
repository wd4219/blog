header-color = #20A0FF
main-color= #D3DCE6
border-radius(n)
  border-radius n
box-shadow(n)
  box-shadow 0 0 n #99A9BF
@keyframes draw-nav-item 
  0% 
    stroke-dasharray: 100,600
    stroke-dashoffset: -215
    stroke-width: 8px
  100% 
    stroke-dasharray: 400
    stroke-dashoffset: 0
    stroke-width: 2px

@keyframes offset {
  to {
    stroke-dashoffset: 0px;
  }
}

@keyframes fadeInDown {
  from {
    opacity: 0;
    transform: translate3d(-50%, -150%, 0);
  }

  to {
    opacity: 1;
    transform: translate3d(-50%, 0, 0);
  }
}

@keyframes fadeInUp {
  from {
    opacity: 1;
    transform: translate3d(-50%, 0, 0);
  }

  to {
    opacity: 0;
    transform: translate3d(-50%, -150%, 0);
  }
}
@keyframes ScaleUp {
  from {
    transform: scale(0.8);
    opacity: 0;
  }

  to {
    transform: scale(1);
    opacity: 1
  }
}
@keyframes bounceInUp {
  from, 60%, 75%, 90%, to {
    animation-timing-function: cubic-bezier(0.215, 0.610, 0.355, 1.000);
  }

  from {
    opacity: 0;
    transform: translate3d(0, 3000px, 0);
  }

  60% {
    opacity: 1;
    transform: translate3d(0, -20px, 0);
  }

  75% {
    transform: translate3d(0, 10px, 0);
  }

  90% {
    transform: translate3d(0, -5px, 0);
  }

  to {
    transform: translate3d(0, 0, 0);
  }
}
.clearfix:after
  content: '.'
  height: 0
  visibility: hidden
  clear: both
  overflow: hidden